function mutation(arr) {
  var str1= arr[0].toLowerCase();
  var str2= arr[1].toLowerCase();
  
//  arr = arr.indexOf(arr[1]); 
  for (var i=0; i < str2.length; i++) {
  var val = str1.indexOf(str2[i]);
  if ( val===-1) {
      return false;
      } 
  
  } 
  return true;
//  return arr;

}

mutation(["hello", "hey"]);
