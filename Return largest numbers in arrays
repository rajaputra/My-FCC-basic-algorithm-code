function largestOfFour(arr) {
  // You can do this!
 var maxArr =[] ;
  
   for (var i=0; i < arr.length; i++) {
     var maxOfFour = 0;
     for (var j=0; j < arr[i].length; j++) {
       if (maxOfFour < arr[i][j]) {
        maxOfFour = arr[i][j];
       }
    }
     maxArr.push(maxOfFour);
  } 
// } );
